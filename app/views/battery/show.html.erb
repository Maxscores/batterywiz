<div class="top-nav table">
  <ul>
    <li>Location <%= image_tag "256_location.png" %> </li>
    <li>Consumption <%= image_tag "256_ecology-lightbulb.png" %></li>
    <li>Solar System <%= image_tag "256_solar-panel.png" %></li>
    <li class="active">System Summary <%= image_tag "256_piggy-bank.png" %></li>
  </ul>
</div>

<div class="row">
  <div class="col col-2">
    <p></p>
  </div>
  <div class="col col-8">
    <div class="row">
      <div class="col col-6">
        <p class="h4">Solar System Summary</p>
        <p class="h6 sans-serif">
          Solar System Capacity: <%= @installation.solar_system.capacity %><br>
          Battery Capacity: <%= @installation.solar_system.capacity %><br>
          Azimuth Angle: <%= @installation.solar_system.azimuth %><br>
          Panel Tilt: <%= @installation.solar_system.tilt %><br>
          Array Type: <%= @installation.solar_system.array_type %><br>
          Module Type: <%= @installation.solar_system.module_type %><br>
          Losses: <%= @installation.solar_system.losses %>
        </p>
      </div>
      <div class="col col-6">
        <p class="h4">Location Summary</p>
        <p class="h6 sans-serif">
          Installation Address: <%= @installation.zipcode %><br>
          Weather Station: <br>
          Station City: <br>
          Station State: <br>
        </p>
      </div>
    </div>
    <div class="row">
      <div class="col col-12" style="background-color: grey; height: 300px;">
        <p class="center h1">GRAPH</p>
      </div>
    </div>
  </div>
</div>
